{
	"apiVersion": 2,
	"name": "coderlift-block/coderlift-countdown",
	"title": "Number Counter",
	"category": "coderlift",
	"icon": "ellipsis",
	"keywords": [ "coderlift", "countup" ],
	"description": "Example block written with ESNext standard and JSX support â€“ build step required.",
	"supports": {
		"html": false
	},
	"textdomain": "coderlift",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",

	"attributes": {
		"alignment": {
			"type": "string",
			"default": "left"
		},	
		"startNumber": {
			"type": "number",
			"default": 0
		},
		"endNumber": {
			"type": "number",
			"default": 100
		},
		"additionalText": {
			"type": "string",
			"source": "html",
			"selector": "h4",
			"default": "Replace Text"	
		},
		"numberPrefix": {
			"type": "string",
			"source": "html",
			"selector": "h2",
			"default": ""	
		},
		"numberSuffix": {
			"type": "string",
			"source": "html",
			"selector": "h2",
			"default": ""	
		}
	}
}